{
    "name": "turso/libsql",
    "description": "libSQL bindings for PHP",
    "type": "library",
    "authors": [
        {
            "name": "Levy A.",
            "email": "levyddsa@gmail.com"
        }
    ],
    "autoload": {
        "classmap": [ "src/" ],
        "psr-4": {
          "Turso\\Libsql\\": "src/",
          "Turso\\Libsql\\Tests\\": "tests/"
        }
    },
    "require": {
        "ext-ffi": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^11",
        "friendsofphp/php-cs-fixer": "^3.64"
    },
    "scripts": {
        "test": [ "@php vendor/bin/phpunit tests --fail-on-warning" ]
    },
    "extra": {
        "files": [
            "lib/universal2-apple-darwin/liblibsql.dylib",
            "lib/x86_64-unknown-linux-gnu/liblibsql.so"
        ]
    },
    "minimum-stability": "dev",
    "license": "MIT"
}
